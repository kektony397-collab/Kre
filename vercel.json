
{
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/manifest.webmanifest",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!api/|sw.js|manifest.webmanifest|_vite/|assets/|favicon.ico|apple-touch-icon.png|mask-icon.svg|pwa-192x192.png|pwa-512x512.png).*)",
      "destination": "/index.html"
    }
  ]
}
